{
  "name": "@discordjs-japan/homepage",
  "private": true,
  "description": "discordjs-japan.orgのソースコード",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "prepare": "node ./scripts/prepare.js",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "format": "prettier --write . && eslint ./app --fix",
    "format:check": "prettier --check . && eslint ./app",
    "generate:css": "tailwindcss -m -i ./styles/app.css -o ./app/styles/tailwind.css",
    "build": "npm run build:css && npm run build:remix",
    "build:remix": "remix build",
    "build:css": "npm run generate:css -- --minify",
    "dev": "run-p build:* && run-p dev:*",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev:css": "npm run generate:css -- --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@remix-run/cloudflare": "^1.12.0",
    "@remix-run/cloudflare-pages": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.4",
    "wrangler": "^2.10.0"
  },
  "volta": {
    "node": "16.19.0"
  }
}
